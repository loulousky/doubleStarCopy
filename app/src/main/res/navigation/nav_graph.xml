<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LHHomeView">

    <fragment
        android:id="@+id/LHHomeView"
        android:name="com.liuhai.doublestars.view.LHHomeView"
        android:label="fragment_lhhome_view"
        tools:layout="@layout/fragment_lhhome_view" >
        <action
            android:id="@+id/action_LHHomeView_to_LHFindListView"
            app:destination="@+id/LHFindListView"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/LHHomeView" />
    </fragment>
    <fragment
        android:id="@+id/LHFindListView"
        android:name="com.example.liuhai.homemodel.view.LHFindListView"
        android:label="LHFindListView" />
</navigation>